.box{
  display: flex;
  flex-direction: column;
}
.vote-star{
  display:inline-block;/*内联元素转换成块元素，并不换行*/
  margin-right:6px;
  width:75px;/*5个星星的宽度*/
  height:15px;/*1个星星的高度*/
  overflow:hidden;
  vertical-align:middle;
  background:url("data:image/png;base64,") repeat-x;
}
.ll{
  display:inline-block;/*内联元素转换成块元素，并不换行*/
  height:15px;/*1个星星的高度*/
  background:url("data:image/png;base64,") repeat-x 00;
}
  .vote-number{
  vertical-align:middle;
  font-family:微软雅黑,Verdana,Geneva, sans-serif;
  font-size:12px;
}
.star-after{
  position: relative;
  width: 75px; 
  top: -15px;   /*向上顶15px高度*/
  line-height: 15px;  /*因为<text>标签有默认的高度，所以要修改<text>标签的高度*/
}
.container{
 display:flex;
 flex-direction:column;
 align-items: center;
}
.submitButton{
  width: 130rpx;
  height: 80rpx;
}